import("graphic","")

init_graphic()
show_window()
x = 1;
y = 0;
sens = 1;
while (1) {
	update()
	fill_window(0,0,0)
	for(i from 0 to 10) {
		for(k from 0 to 10) {
			set_pixel(x+ i, y + k, 0xFF, 0, 0xFF)
		}
	}
	if (sens == 1) {
		x += 1
	}
	if (sens == -1) {
		x -= 1
	}
	if (x > get_width() - 10) {
		sens = -1
		y += 10
	}
	if (x == 0) {
		sens = 1
		y += 10
	}
	update_window()
	sleep(0.01)
}
update_window()

while (1) {
	update()
}